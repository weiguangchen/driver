page {
  height: 100vh;
  display: flex;
  flex-direction: column;
}
.navbar-content {
  padding-left: 46px;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.stat-total {
  padding: 24rpx;
  display: flex;
  background-color: #ffffff;
}
.stat-total .total-item {
  flex: 1;
  padding: 32rpx 32rpx 28rpx;
  background: #02B72E;
  border-radius: 24rpx;
}
.stat-total .total-item .title {
  font-weight: bold;
  font-size: 28rpx;
  color: var(--title-color);
  line-height: 40rpx;
}
.stat-total .total-item .number {
  font-weight: bold;
  font-size: 36rpx;
  line-height: 64rpx;
}
.stat-total .left {
  margin-right: 22rpx;
  background: url(data:image/png;base64,) no-repeat right 12rpx top 12rpx/72rpx 72rpx, rgba(2, 183, 46, 0.08);
}
.stat-total .left .number {
  color: var(--main-color);
}
.stat-total .right {
  background: url(data:image/png;base64,) no-repeat right 12rpx top 12rpx/72rpx 72rpx, rgba(252, 126, 44, 0.08);
}
.stat-total .right .number {
  color: #FC7E2C;
}
.list-wrapper {
  padding: 24rpx 24rpx calc(var(--safe-area-inset-bottom) + 24rpx);
}
.list-wrapper .title {
  padding: 8rpx 8rpx 20rpx;
  font-weight: bold;
  font-size: 32rpx;
  color: var(--title-color);
  line-height: 40rpx;
}