.bill-wrapper{padding:28rpx;background:#fff;border-radius:24rpx;margin-bottom:20rpx}.bill-wrapper .mfrs{display:flex;align-items:center;justify-content:space-between;color:var(--title-color);line-height:48rpx}.bill-wrapper .mfrs .name{font-weight:700;font-size:32rpx}.bill-wrapper .mfrs .date{font-size:26rpx;color:var(--sub-color)}.bill-wrapper .cate{font-size:26rpx;color:var(--sub-color);line-height:48rpx;margin-bottom:20rpx}.bill-wrapper .location-wrapper{padding:24rpx 20rpx;background:var(--page-bg);border-radius:16rpx;margin-bottom:28rpx}.bill-wrapper .location-wrapper .line{position:absolute;height:24rpx;top:34rpx;left:6rpx}.bill-wrapper .location-wrapper .line .dot{width:4rpx;height:4rpx;border-radius:50%;background-color:#c8d4df}.bill-wrapper .location-wrapper .line .dot:not(:last-child){margin-bottom:6rpx}.bill-wrapper .location-wrapper .from,.bill-wrapper .location-wrapper .to{display:flex;align-items:center;font-weight:500;font-size:26rpx;line-height:36rpx}.bill-wrapper .location-wrapper .from .city,.bill-wrapper .location-wrapper .to .city{color:var(--title-color);margin-right:16rpx;max-width:120rpx}.bill-wrapper .location-wrapper .from .name,.bill-wrapper .location-wrapper .to .name{flex:1;color:var(--content-color)}.bill-wrapper .location-wrapper .from .dis,.bill-wrapper .location-wrapper .to .dis{color:var(--main-color);margin-left:12rpx}.bill-wrapper .location-wrapper .from{position:relative;margin-bottom:20rpx}
