.list-wrapper{padding:24rpx}.list{padding:0 24rpx;background:#fff;border-radius:24rpx}.list .item{position:relative;padding:36rpx 0 36rpx 36rpx;display:flex;align-items:center;justify-content:space-between;font-size:28rpx;color:var(--title-color);line-height:36rpx}.list .item .label{margin-right:26rpx}.list .item:before{content:"";display:block;width:12rpx;height:12rpx;border-radius:50%;background-color:var(--main-color);position:absolute;left:0;top:50%;transform:translateY(-50%)}
