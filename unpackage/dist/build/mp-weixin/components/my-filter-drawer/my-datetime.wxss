.date-range{display:flex;align-items:center;justify-content:space-between;margin-bottom:24rpx;padding:0 10rpx}.date-range .start-date,.date-range .end-date{width:300rpx;flex:none;display:flex;align-items:center;justify-content:space-between;color:var(--intr-color);padding:0 32rpx;height:72rpx;background:#fff;border-radius:36rpx;border:2rpx solid var(--divider);font-size:26rpx;line-height:48rpx}.date-range .start-date--active,.date-range .end-date--active{opacity:.45}.date-range .start-date.active,.date-range .end-date.active{color:var(--title-color)}
