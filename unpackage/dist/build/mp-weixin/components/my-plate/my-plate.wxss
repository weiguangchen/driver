.my-plate{width:-webkit-fit-content;width:fit-content;display:flex;padding:4rpx;height:64rpx;border-radius:12rpx;color:var(--title-color);font-weight:700}.my-plate .inner{font-family:misans;flex:1;display:flex;border-radius:10rpx;overflow:hidden;border:2rpx solid var(--title-color)}.my-plate .inner .left{padding:0 10rpx;position:relative;display:flex;align-items:center}.my-plate .inner .left:after{content:"";display:block;position:absolute;width:8rpx;height:8rpx;border-radius:50%;background-color:var(--title-color);top:50%;right:0;transform:translate(50%,-50%)}.my-plate .inner .right{display:flex;align-items:center;padding:0 10rpx}.my-plate.yellow{background:#ffbd00}.my-plate.kelly{background-color:#ffbd00}.my-plate.kelly .left{background-color:transparent}.my-plate.kelly .right{background-color:#00ec7b}.my-plate.blue{background-color:#3773ff;color:#fff}.my-plate.blue .inner{border:2rpx solid #ffffff}.my-plate.blue .inner .left:after{background-color:#fff}.my-plate.gren{background:linear-gradient(180deg,#f8f8f8,#00ec7b)}
